{
  "extends": "@backstage/cli/config/tsconfig.json",
  "include": [
    "src"
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "dist-types",
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
